#/etc/apt/apt.conf.d/10proxy (UZ proxy) 
Acquire::http::Proxy "http://proxy.uz.ac.zw:3128";

#/etc/network/interfaces
allow-hotplug wlan0
iface wlan0 inet manual
wpa-roam /etc/wpa_supplicant/wpa_supplant.conf
iface default inet dhcp

#/etc/wpa_supplicant/wpa_supplicant.conf
network={
  ssid="<network name>"
  psk="<password>"
}

#scan network
nmap -sn 192.168.1.0/24
